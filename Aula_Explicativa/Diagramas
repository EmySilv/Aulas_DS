//Aula de DS - 05/08/2022
public class Pessoa {
   private String nome;
   public void setNome(String nome){
   this.nome = nome;
   }
   public String getNome(){
   return(nome);
   }
}


public class PessoaFisica extends Pessoa{
   private String rg;
   public void setNome(String rg){
   this.rg = rg;
   }
   public String getRg(){
   return(rg);
   }
}
